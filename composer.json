{
    "name": "permittedleader/tiffey-tables",
    "description": "Tables for Laravel using the Tiffey layout engine",
    "type": "project",
    "require": {
        "laravel/framework": "^10.0",
        "livewire/livewire": "^3.0",
        "maatwebsite/excel": "^3.1",
        "permittedleader/tiffey": "^1.4.4",
        "permittedleader/flash-messages": "^2.0.2",
        "spatie/once": "^3.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-laravel": "^2.3",
        "pestphp/pest-plugin-livewire": "^2.1",
        "orchestra/testbench": "^8.21"
    },
    "authors": [
        {
            "name": "Myles Smith",
            "email": "159888845+PermittedLeader@users.noreply.github.com"
        }
    ],
    "license": "GNU GPLv3",
    "autoload": {
        "psr-4": {
            "Permittedleader\\Tables\\": "src/"
        }
    },
    "repositories": {
        "flash-messages": {
            "type": "vcs",
            "url": "https://github.com/PermittedLeader/flash-messages.git"
        },
        "tiffey": {
            "type": "vcs",
            "url": "https://github.com/PermittedLeader/tiffey.git"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Permittedleader\\Tables\\TablesServiceProvider"
            ]
        }
    }
}
